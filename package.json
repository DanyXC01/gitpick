{
  "name": "gitpick",
  "version": "1.2.0",
  "description": "CLI tool to find and analyze GitHub repos for contributors",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "gitpick": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc && node dist/index.js",
    "start": "node dist/index.js",
    "search": "node dist/index.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": ["github", "opensource", "cli", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "octokit": "^3.1.2",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "cli-progress": "^3.12.0",
    "open": "^10.0.3",
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/inquirer": "^9.0.7",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}
